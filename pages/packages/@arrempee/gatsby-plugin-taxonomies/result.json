      "npmPackage": {
        "name": "@arrempee/gatsby-plugin-taxonomies",
        "description": "A Gatsby plugin that allows for Hugo-esque Taxonomies on arbitrary nodes.",
        "keywords": [
          "gatsby",
          "gatsby-plugin"
        ],
        "lastPublisher": {
          "name": "arrempee",
          "avatar": "https://gravatar.com/avatar/860145a53106371cf6644e3fb56e9c06"
