          "gatsby",
          "gatsby-plugin",
          "gatsby-source-plugin"
        ],
        "lastPublisher": {
          "name": "duodealer",
          "avatar": "https://gravatar.com/avatar/fde6d4a7fa52957faa850d148543859b"
