        "name": "gatsby-source-mysql2",
        "description": "Source plugin for pulling data into Gatsby from MySQL database using MySQL2.",
        "keywords": [
          "gatsby",
          "gatsby-plugin",
          "mysql"
        ],
        "lastPublisher": {
          "name": "imedadel",
          "avatar": "https://gravatar.com/avatar/43227e0c55c651f8c991ccedea42e6cd"
